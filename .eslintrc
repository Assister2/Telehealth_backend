{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["warn", { "varsIgnorePattern": "VARIABLE_NAME"}],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "linebreak-style": 0,
    "indent": "off"
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": [
    "airbnb-base"
  ]
}
